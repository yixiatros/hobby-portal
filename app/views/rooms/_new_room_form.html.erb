<%= form_with model: @room, url: rooms_create_path, class: "d-flex" do |form| %>
  <div>
    <%= form.text_field :name, class: "new-room-input form-control", autocomplete: 'off' %>
  </div>
  <div class="create-room-button-div">
    <%= form.submit "Create Room", data: { disable_with: false }, class: "login-button btn"%>
  </div>
<% end %>